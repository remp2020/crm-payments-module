{
  "name": "remp/crm-payments-module",
  "description": "CRM Payments Module",
  "type": "library",
  "homepage": "https://remp2020.com",
  "license": "MIT",
  "require": {
    "php": "^8.0",
    "ext-soap": "*",
    "php-http/curl-client": "^2.2.1",
    "tomaj/bank-mails-parser": "^2.7",
    "tomaj/imap-mail-downloader": "^1.2.0",
    "rootpd/omnipay-csob": "~4.2",
    "rootpd/omnipay-paypal-reference": "^4.0.0",
    "rootpd/openpgp-php": "~0.5",
    "tomaj/omnipay-tatra": "4.2"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Crm\\PaymentsModule\\": "src/"
    },
    "classmap": ["src/"]
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "rootpd/openpgp-php",
        "version": "0.5.1",
        "source": {
          "url": "https://github.com/rootpd/openpgp-php.git",
          "type": "git",
          "reference": "0.5.1"
        },
        "autoload": {
          "classmap": ["lib/"]
        }
      }
    }
  ]
}
